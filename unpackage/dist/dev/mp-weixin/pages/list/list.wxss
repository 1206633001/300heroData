.loading {
	padding-top: 50rpx;
	-webkit-transform: translateY(-250rpx);
	    -ms-transform: translateY(-250rpx);
	        transform: translateY(-250rpx);
	-webkit-transition: -webkit-transform .3s;
	transition: -webkit-transform .3s;
	-o-transition: transform .3s;
	transition: transform .3s;
	transition: transform .3s, -webkit-transform .3s;
	height: 250rpx;
}
.loading-img {
	width: 100rpx;
	height: 152rpx;
	margin: 0 auto;
}
.loading-title {
	color: #c9b563;
	text-align: center;
}
.show-loading {
	-webkit-transform: translate(0rpx);
	    -ms-transform: translate(0rpx);
	        transform: translate(0rpx);
}
.hide-loading {
	-webkit-transform: translateY(-250rpx);
	    -ms-transform: translateY(-250rpx);
	        transform: translateY(-250rpx);
}
.page {
	position: relative;
	top: 0rpx;
	opacity: 0;
	-webkit-transition: opacity .3s;
	-o-transition: opacity .3s;
	transition: opacity .3s;
}
.hide-page {
	opacity: 0;
}
.show-page {
	opacity: 1;
}

